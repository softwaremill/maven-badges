  "license": "Apache-2.0",
  "dependencies": {
    "@types/redis": "^4.0.11",
    "axios": "^0.26.1",
    "express": "^4.17.3",
    "heroku-logger": "^0.3.3",
    "redis": "^4.0.6"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/express": "^4.17.13",
    "@types/heroku-logger": "^1.0.1",
    "@types/mocha": "^9.1.0",
    "@types/node": "~17.0.24",
    "@types/redis-mock": "^0.17.1",
    "@types/supertest": "^2.0.12",
    "axios-mock-adapter": "^1.20.0",
    "chai": "~4.3.6",
    "mocha": "^9.2.2",
    "nodemon": "~2.0.15",
    "redis-mock": "^0.56.3",
    "supertest": "^6.2.2",
    "typescript": "~4.6.3"
  }
