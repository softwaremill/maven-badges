    "start": "npm run tsc:build && node ./dist/main.js"
  },
  "author": "marcin.baraniecki@softwaremill.com",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/redis": "^2.8.6",
    "axios": "^0.18.0",
    "express": "^4.16.3",
    "redis": "^2.8.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "~10.11.3",
    "@types/redis-mock": "^0.17.0",
    "@types/supertest": "^2.0.6",
    "axios-mock-adapter": "^1.15.0",
    "chai": "~4.2.0",
    "mocha": "^5.2.0",
    "nodemon": "~1.18.4",
    "redis-mock": "^0.33.0",
    "supertest": "^3.3.0",
    "typescript": "~3.1.1"
  }
}
