{
  "name": "maven-badges",
  "version": "1.0.0",
  "description": "A node.js implementation of maven-badges service.",
  "engines": {
    "node": "12.18.3"
  },
  "scripts": {
    "tsc:watch": "tsc -w",
    "tsc:build": "tsc",
    "serve": "nodemon --inspect --watch dist/ -d 1 ./dist/main.js",
    "test": "npm run tsc:build && mocha dist/test --timeout 15000 --exit",
    "start": "node ./dist/main.js",
    "postinstall": "npm run tsc:build"
  },
  "author": "marcin.baraniecki@softwaremill.com",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/redis": "^2.8.31",
    "axios": "^0.21.1",
    "express": "^4.17.1",
    "heroku-logger": "^0.3.3",
    "redis": "^3.1.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/express": "^4.17.13",
    "@types/heroku-logger": "^1.0.1",
    "@types/mocha": "^8.2.3",
    "@types/node": "~16.4.0",
    "@types/redis-mock": "^0.17.0",
    "@types/supertest": "^2.0.11",
    "axios-mock-adapter": "^1.19.0",
    "chai": "~4.3.4",
    "mocha": "^9.0.2",
    "nodemon": "~2.0.12",
    "redis-mock": "^0.56.3",
    "supertest": "^6.1.4",
    "typescript": "~4.3.5"
  }
